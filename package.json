{
  "name": "cicd-boot",
  "version": "1.0.5",
  "bin": {
    "pipeline": "bin/pipeline-init.ts"
  },
  "scripts": {
    "prepare": "if [ -z $CODEBUILD_BUILD_ID ] && [ -z $GITHUB_ACTIONS ]; then husky install; fi",
    "build": "tsc && tsc --build --clean && npm run lint",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "npx aws-cdk@2.92.0",
    "generate-checksum": "ts-node ./scripts/package-checksum-generator.ts",
    "audit": "npm run audit-nodejs && npm run audit-python",
    "audit-nodejs": "npx better-npm-audit@3.7.3 audit --level=high --registry='https://registry.npmjs.org/'",
    "audit-python": "./scripts/check-deps-python.sh",
    "lint": "npx eslint . --ext .ts --max-warnings 0",
    "lint-fix": "npx eslint . --ext .ts --fix",
    "validate": "npx ts-node ./scripts/package-checksum-validator.ts"
  },
  "config": {
    "applicationName": "CICDBoot",
    "cdkQualifier": "cicdboot",
    "repositoryName": "cicd-boot",
    "repositoryType": "CODECOMMIT",
    "cicdVpcType": "NO_VPC"
  },
  "devDependencies": {
    "@types/jest": "29.5.4",
    "@types/node": "20.6.0",
    "@types/prettier": "2.6.0",
    "@types/source-map-support": "0.5.7",
    "@typescript-eslint/eslint-plugin": "6.7.0",
    "@typescript-eslint/parser": "6.7.0",
    "eslint": "8.49.0",
    "eslint-plugin-import": "2.28.1",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@cloudcomponents/cdk-pull-request-approval-rule": "2.1.0",
    "@cloudcomponents/cdk-pull-request-check": "2.1.0",
    "@types/uuid": "9.0.3",
    "aws-cdk-lib": "2.92.0",
    "cdk-monitoring-constructs": "5.10.1",
    "cdk-nag": "2.27.129",
    "constructs": "10.2.70",
    "source-map-support": "0.5.21",
    "uuid": "9.0.1"
  },
  "overrides": {
    "http-cache-semantics": "4.1.1",
    "semver": "7.5.3",
    "@babel/traverse": "7.23.2"
  }
}
